   "source": [
    "import torch\n",
    "x = torch.arange(0, 5, 1.0, requires_grad = True)\n",
    "y = x * x\n",
    "u = y.detach()\n",
    "z = u * x\n",
    "z.sum().backward()\n",
    "x.grad, y\n",
    "\n"
   ]
